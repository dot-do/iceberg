{
  "name": "iceberg.do",
  "version": "0.2.0",
  "description": "Iceberg REST Catalog as a Service - managed Iceberg catalog on Cloudflare Workers",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src"
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "iceberg",
    "catalog",
    "cloudflare",
    "workers",
    "durable-objects",
    "r2",
    "rest-api"
  ],
  "author": "dotdo",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dot-do/iceberg"
  },
  "homepage": "https://iceberg.do",
  "dependencies": {
    "@dotdo/iceberg": "workspace:*",
    "hono": "^4.0.0",
    "json-bigint": "^1.0.0",
    "oauth.do": "^0.1.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240208.0",
    "@types/json-bigint": "^1.0.4",
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0",
    "vitest": "^3.2.4",
    "wrangler": "^3.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
