{
  "name": "iceberg-workspace",
  "private": true,
  "workspaces": [
    "core",
    "service"
  ],
  "scripts": {
    "build": "pnpm -r build",
    "test": "pnpm -r test",
    "test:e2e": "vitest run --config e2e/vitest.config.ts e2e/iceberg-do.test.ts",
    "test:e2e:watch": "vitest --config e2e/vitest.config.ts e2e/iceberg-do.test.ts",
    "test:conformance": "vitest run --config e2e/vitest.config.ts e2e/conformance/",
    "test:conformance:rck": "bash e2e/conformance/run-rck.sh",
    "test:conformance:pyiceberg": "bash e2e/conformance/run-pyiceberg-tests.sh",
    "lint": "pnpm -r lint",
    "dev": "pnpm -r dev"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "pnpm@9.0.0",
  "devDependencies": {
    "iceberg-js": "^0.8.1",
    "typescript": "^5.3.0",
    "vitest": "^3.2.4"
  }
}
